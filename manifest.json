{
  "manifest_version": 2,
  "name": "YouTube 360 Gaze",
  "version": "0.0.1",

  "description": "Gaze tracking input for YouTube.",

  "browser_action": {
    "default_icon": "./icons/icon.png",
    "default_popup": "./popup.html",
    "default_title": "Uses gaze tracking to pan video input."
  },

  "background": {
    "scripts": [
      "./scripts/background_script.js"
    ],
    "persistent": false
  },
  "short_name": "YT Gaze"
}
